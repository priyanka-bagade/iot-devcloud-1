{
	"job":
	{
		"command": "qsub JOB_ENV_SCRIPT -l nodes=1:NODE:ARCH -F \"RESULTS\"",
		"output_type": ".png",
		"results_path": "results/",
		"results_defines" : "RESULTS",
		"progress_indicators":
		[
			{
				"file_name": "i_progress.txt",
				"title": "Inference Progress"
			}
		],
		"plots":
		[
			{
				"title" : "Inference Time",
				"type" : "time",
				"ylabel" : "Time in seconds",
				"xlabel" : "Architectures",
				"selector" : ["Target Environment","Target node", "Target architecture"]
			},
			{
				"title" : "Frames per second",
				"type" : "fps",
				"ylabel" : "Number of frames per second",
				"xlabel" : "Architectures"
			}
		]
	},
	"inputs":
	[
		{
			"Target_node":
			{
				"display_name": "Target node",
				"type": "select",
				"default" : "tank-870",
				"defines" : ["NODE"],
				"options": [
				{
					"name": "Select Node",
					"dummy": "True",
					"controls":
					{
						"Target_arch":["Intel Core i7-8665UE CPU", "Intel Core i5-7500T CPU", "Intel Core i5-6500TE CPU", "Intel Xeon CPU"]
					}
				},
				{
					"name": "6th Gen Intel Core CPU",
					"defines" :
					{
						"NODE" : "idc001skl"
					},
					"controls":
					{
						"Target_arch":["Intel Core i5-6500TE CPU"]
					}
				},
				{
					"name": "7th Gen Intel Core CPU",
					"defines" :
					{
						"NODE" : "idc006kbl"
					},
					"controls":
					{
						"Target_arch":["Intel Core i5-7500T CPU"]
					}
				},
				{
					"name": "8th Gen Intel Core CPU",
					"defines" :
					{
						"NODE" : "idc014upxa10fx1"
					},
					"controls":
					{
						"Target_arch":["Intel Core i7-8665UE CPU"]
					}
				},
				{
					"name": "6th Gen Intel Xeon CPU",
					"defines" :
					{
						"NODE" : "idc007xv5"
					},
					"controls":
					{
						"Target_arch":["Intel Xeon CPU"]
					}
				}
				]
			}
		},
		{
			"Target_arch":
			{
				"display_name": "Target architecture",
				"type": "select",
				"default" : "nodes=1:i5-6500te",
				"defines" : ["ARCH", "TARGET", "FP_MODEL"],
				"options": [
					{
						"name": "Select Architecture",
						"dummy": "True"
					},
					{
						"name":"Intel Core i5-6500TE CPU",
						"defines":
						{
							"ARCH": "i5-6500te",
							"TARGET": "CPU"
						}
					},
					{
						"name":"Intel Core i5-7500T CPU",
						"defines":
						{
							"ARCH": "i5-7500t",
							"TARGET": "CPU"
						}
					},
					{
						"name":"Intel Core i7-8665UE CPU",
						"defines":
						{
							"ARCH": "i7-8665ue",
							"TARGET": "CPU"
						}
					},
					{
						"name": "Intel Xeon CPU",
						"defines":
						{
							"ARCH": "e3-1268l-v5",
							"TARGET": "CPU"
						}
					}
				]
			}
		},
		{
			"Target Environment":
			{
				"display_name" : "Target Environment",
				"type" : "select",
				"default" : "Stock",
				"defines" : ["JOB_ENV_SCRIPT"],
				"options": [
					{
						"name": "Select Env",
						"dummy": "True"
					},
					{
						"name": "Stock Tensorflow (stocktf) Env",
						"defines":
						{
							"JOB_ENV_SCRIPT": "healthcare_job_stocktf.sh"
						}
					},
					{
						"name": "Intel Optimized Tensorflow (mkltf) Env",
						"defines":
						{
							"JOB_ENV_SCRIPT": "healthcare_job_mkltf.sh"
						}
					}
				]
			}
		}
	]
}